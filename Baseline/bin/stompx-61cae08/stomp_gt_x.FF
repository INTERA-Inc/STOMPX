      ISUB_LOG = ISUB_LOG+1
      SUB_LOG(ISUB_LOG) = '/READ_PROP_GT'
!
!---  Allocate local memory for property arrays ---
!
      CALL ALLOC_PROP
!
!---  Allocate local memory for lookup table arrays ---
!
      CALL ALLOC_TABL
!
!---  Set local starting point for local copies of nodal variables  ---
!
      NC = 0
      DO I = 1,ID
        NC = NC + NFCGC(I)
      ENDDO
!
