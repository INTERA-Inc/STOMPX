      SUBROUTINE SP_GT( ASL_F,ASL_M,ASLX,ASLMINX,ESLX,ESGTX,ESGTMX,
     &  PGX,PLX,BTGLX,SGX,SGTX,SLX,SLFX,SLMX,SLRX,INDX,N )
