!     processors  ---
!
      NVAR = LTBL
      OFFSET = IOFFSET + NBYTB
      IOFFSET = IOFFSET + NVAR*NBYTR + 2*NBYTB
      CALL MPI_FILE_READ_AT( IRD,OFFSET,TBLY,NVAR,MPI_REAL8,
     &  STATUS,IERR)
