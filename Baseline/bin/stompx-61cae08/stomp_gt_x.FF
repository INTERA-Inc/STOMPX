      CALL RKGS_GT( ASL_F,ASL_M,ASLX,ASLMINX,BTGLX,ESGTX,PGX,PLX,RKGX,
     &  SGX,SGTX,SLX,N )
!
!---  Reset subroutine string sequence  ---
!
      ISUB_LOG = ISUB_LOG-1
!
!---  End of KSP_GT group  ---
!
      RETURN
      END

