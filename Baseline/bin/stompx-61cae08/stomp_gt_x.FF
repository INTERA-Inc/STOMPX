!
      ISUB_LOG = ISUB_LOG+1
      SUB_LOG(ISUB_LOG) = '/KSP_GT'
!
!---  Aqueous and gas saturation  ---
!
      IF( NPHAZ(2,N).EQ.4 ) THEN
        ASL_F = 0.D+0
        ASL_M = 0.D+0
        ASLX = 0.D+0
        ASLMINX = 0.D+0
        ESLX = 0.D+0
        ESGTX = 0.D+0
        SGTX = 0.D+0
        SLX = 0.D+0
