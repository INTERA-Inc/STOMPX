          SBFS(NCS+13) = YMHGN(2,NLSGC(M+MCS))
          SBFS(NCS+14) = YMHGO(2,NLSGC(M+MCS))
          SBFS(NCS+15) = ZMCA(2,NLSGC(M+MCS))
          SBFS(NCS+16) = ZMCN(2,NLSGC(M+MCS))
          SBFS(NCS+17) = ZMCO(2,NLSGC(M+MCS))
          NCS = NCS + NPVX
